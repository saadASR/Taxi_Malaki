<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background circle -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#FDB815" /> <!-- Background color -->
            <size android:width="36dp" android:height="36dp" /> <!-- Adjust size -->
        </shape>
    </item>

    <!-- Icon in the center -->
    <item>
        <bitmap
            android:src="@drawable/map_iccc"
            android:gravity="center" />
    </item>
</layer-list>
